package aplicacao;

import javax.persistence.EntityManager;
import javax.persistence.EntityManagerFactory;
import javax.persistence.Persistence;

import dominio.Pessoa;

public class Programa {

	public static void main(String[] args) {
		Pessoa p1 = new Pessoa(1, "Uilson Alves", "uilsongmail.com");
		Pessoa p2 = new Pessoa(1, "Alessandro Reinaldo", "alegmail.com");
		Pessoa p3 = new Pessoa(1, "Maria Silva", "mariagmail.com");
		Pessoa p4 = new Pessoa(1, "Regina Castro", "reginagmail.com");
		
		EntityManagerFactory emf = Persistence.createEntityManagerFactory("aula-jpa");
		EntityManager em = emf.createEntityManager();
		
		
		System.out.println(p1);
		System.out.println(p2);
		System.out.println(p3);
		System.out.println(p4);
	}
}
